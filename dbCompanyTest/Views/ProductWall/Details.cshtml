            @Html.DisplayFor(model => model.商品顏色.商品顏色id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.圖片位置)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.圖片位置.圖片位置id)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
