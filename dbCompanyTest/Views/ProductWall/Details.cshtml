@model dbCompanyTest.Models.ProductDetail

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a asp-area="" asp-controller="Home" asp-action="Index">首頁</a></li>
        <li class="breadcrumb-item"><a href="#">@ViewBag.商品分類</a></li>
        <li class="breadcrumb-item active" aria-current="page">@ViewBag.Productname</li>
    </ol>
</nav>
<div>
    <h4>ProductDetail</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.商品數量)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.商品數量)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.商品編號)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.商品編號)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.商品分類.商品分類id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.商品尺寸)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.商品尺寸.商品尺寸id)
